algoritmo "soma_5_10"
// Função :
// Autor :Guilherme
// Data : 15/08/2018
// Seção de Declarações 
var
  num1 : inteiro
  num2 : inteiro
  res : inteiro
inicio
      //processamento
      num1 <- 5;
      num2 <- 10;
      res <- num1 + num2;
      
      //saida
      escreval ("resultado: ", res)
fimalgoritmo
